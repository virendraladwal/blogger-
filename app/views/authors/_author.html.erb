<div id="<%= dom_id author %>">
  <p>
    <strong>Username:</strong>
    <%= author.username %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= author.email %>
  </p>

  <p>
    <strong>Password:</strong>
    <%= author.password %>
  </p>

  <p>
    <strong>Password confirmation:</strong>
    <%= author.password_confirmation %>
  </p>

</div>
