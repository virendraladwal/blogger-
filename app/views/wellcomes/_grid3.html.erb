<div class="w3-row-padding" id="about">
    <div class="w3-center w3-padding-64">
      <span class="w3-xlarge w3-bottombar w3-border-dark-grey w3-padding-16">Who We Are</span>
    </div>
    <% @weare.each do |pro|%>
    <div class="w3-third w3-margin-bottom">
      <div class="w3-card-4">
       <%#img src="/w3images/team1.jpg" alt="John" style="width:100%"%>
         <%= image_tag 'photo-1661956600684-97d3a4320e45.jpeg', style: "width:100%" %> 
          <div class="w3-container">
          <h3><%= pro.name %></h3>
          <p class="w3-opacity"><%= pro.sub_title %></p>
          <p><%= pro.description %></p>
          <p><button class="w3-button w3-light-grey w3-block">Contact</button></p>
        </div>
      </div>
    </div>
    <%end%>
</div>
